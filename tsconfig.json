{
    "compileOnSave": false,
    "compilerOptions": {
        "module": "amd",
        "target": "es6",
        "strictNullChecks": true,
        "alwaysStrict": true,
        "noUnusedParameters": true,
        "jsx": "react",
        "skipLibCheck": true,
        "noImplicitReturns": true,
        "sourceMap": true,
        "noUnusedLocals": true,
        "allowUnreachableCode": false,
        "experimentalDecorators": true,
        "moduleResolution": "node",
        "types": [
            "mendix-client"
        ],
    },
}